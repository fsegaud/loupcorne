<?xml version="1.0" encoding="utf-8"?>
<ArrayOfSimObject xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">

	<SimObject Name="King" Class="Player">
	  <SimProperty Name="MaxHealth" BaseValue="10" />
	  <SimProperty Name="Speed" BaseValue="10" />
	  <SimProperty Name="Attack" BaseValue="10" />
	  <SimProperty Name="Defence" BaseValue="10" />
	  <SimProperty Name="CooldownReductionRatio" BaseValue="1" />
	  <SimProperty Name="Alignement" BaseValue="0" />
	  <SimProperty Name="DamageReductionRatio" BaseValue="1" />
	  <SimProperty Name="AttackRange" BaseValue="3" />
	</SimObject>

	<SimObject Name="Peon" Class="Friend">
	  <SimProperty Name="MaxHealth" BaseValue="7" />
	  <SimProperty Name="Speed" BaseValue="3.5" />
	  <SimProperty Name="Defence" BaseValue="7" />
	  <SimProperty Name="AlignmentRewardOnDeath" BaseValue="-1" />
	</SimObject>

	<SimObject Name="Guard" Class="Foe">
	  <SimProperty Name="MaxHealth" BaseValue="10" />
	  <SimProperty Name="Speed" BaseValue="5" />
	  <SimProperty Name="Attack" BaseValue="10" />
	  <SimProperty Name="Defence" BaseValue="10" />
	   <SimProperty Name="AttackRange" BaseValue="2" />
	  <SimProperty Name="AlignmentRewardOnDeath" BaseValue="1" />
	</SimObject>

</ArrayOfSimObject>